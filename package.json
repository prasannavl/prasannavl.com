{
  "name": "prasannavl.com",
  "version": "1.0.0",
  "description": "Prasanna's Weblog",
  "main": "index.js",
  "scripts": {
    "ready-env": "typings install",
    "clean": "rimraf build/* artifacts/*",
    "build": "npm run clean && npm run generate-content && npm run webpack",
    "build:p": "npm run clean && npm run generate-content && npm run webpack:p",
    "webpack": "webpack --colors --display-chunks --progress",
    "webpack:p": "webpack --colors --display-chunks --production --progress",
    "watch": "webpack --watch --colors --display-chunks",
    "dev": "npm run generate-content && webpack-dev-server --inline --hot --no-info",
    "dev-server": "webpack-dev-server --inline --hot --no-info",
    "dev-nopublish": "babel-node ./content/manager.js run-nopublish && webpack-dev-server --inline --hot --no-info",
    "serve": "babel-node ./tools/server.js --run",
    "serve:p": "babel-node ./tools/server.js --run --production",
    "serve-static": "babel-node ./tools/server.js --run --static",
    "start": "npm run build && npm run serve",
    "start:p": "npm run build:p && npm run serve:p",
    "generate-content": "babel-node ./content/manager.js run-nopublish",
    "generate-routes": "babel-node ./tools/generateRoutes.js --production",
    "generate-pages": "babel-node ./tools/generatePages.js --production",
    "generate-sitemap": "babel-node ./tools/generateSitemap.js --production",
    "generate-all": "npm run generate-routes && npm run generate-sitemap && npm run generate-pages",
    "build-static": "npm run build && npm run generate-all",
    "build-static:p": "npm run build:p && npm run generate-all",
    "start-static": "npm run build-static && npm run serve",
    "start-static:p": "npm run build-static:p && npm run serve:p",
    "displace-sourcemaps": "babel-node ./tools/displaceSourceMaps.js",
    "test-play": "babel-node ./node_modules/tape/bin/tape -r \"ts-node/register\" ./tests/play.ts",
    "test-file": "babel-node ./node_modules/tape/bin/tape -r \"ts-node/register\"",
    "test": "babel-node ./node_modules/tape/bin/tape -r \"ts-node/register\" ./tests/**/*"
  },
  "author": "Prasanna V. Loganathar",
