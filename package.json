{
  "name": "prasannavl.com",
  "version": "1.0.0",
  "description": "Personal website",
  "main": "index.js",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf build/*",
    "build": "npm run clean && npm run generate-content && npm run webpack",
    "build:p": "npm run clean && npm run generate-content && npm run webpack:p",
    "webpack": "webpack --colors --display-chunks --progress",
    "webpack:p": "webpack --colors --display-chunks --production --progress",
    "watch": "webpack --watch --colors --display-chunks",
    "dev": "npm run generate-content && webpack-dev-server --inline --hot --display-chunks --progress",
    "serve": "./node_modules/.bin/babel-node ./tools/server.js --run",
    "serve:p": "./node_modules/.bin/babel-node ./tools/server.js --run --production",
    "serve-static": "./node_modules/.bin/babel-node ./tools/server.js --run --static",    
    "start": "npm run build && npm run serve",
    "start:p": "npm run build:p && npm run serve:p",
    "generate-content": "./node_modules/.bin/babel-node ./content/manager.js",
    "generate-routes": "./node_modules/.bin/babel-node ./tools/generateRoutes.js --production",
    "generate-pages": "./node_modules/.bin/babel-node ./tools/generatePages.js --production",
    "generate-all": "npm run generate-routes && npm run generate-pages",
    "build-static": "npm run build && npm run generate-all",
    "build-static:p": "npm run build:p && npm run generate-all",
    "start-static": "npm run build-static && npm run serve",
    "start-static:p": "npm run build-static:p && npm run serve:p",
    "displace-sourcemaps": "./node_modules/.bin/babel-node ./tools/displaceSourceMaps.js",
    "test-play": "./node_modules/.bin/babel-node ./node_modules/tape/bin/tape -r \"ts-node/register\" ./tests/play.ts",
    "test-file": "./node_modules/.bin/babel-node ./node_modules/tape/bin/tape -r \"ts-node/register\"",
    "test": "./node_modules/.bin/babel-node ./node_modules/tape/bin/tape -r \"ts-node/register\" ./tests/**/*"
  },
